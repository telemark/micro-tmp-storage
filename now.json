{
  "version": 2,
  "name": "micro-tmp-storage",
  "scope": "telemark",
  "alias": [
    "tmp.storage.service.t-fk.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@azure-cosmos-felles-connection",
    "MONGODB_COLLECTION": "tmpstorage",
    "MONGODB_NAME": "felles"
  },
  "builds": [
    { 
      "src": "/index.js",
      "use": "@now/node" 
    },
    {
      "src": "*.md",
      "use": "@now/md"
    }
  ],
  "routes": [
    { "src": "/docs",
      "dest": "/README.html"
    },
    { "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
