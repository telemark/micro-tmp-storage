{
  "name": "micro-tmp-storage",
  "description": "Temporary storage",
  "version": "1.3.3",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "storage",
    "temporary",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-tmp-storage.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-tmp-storage/issues"
  },
  "homepage": "https://github.com/telemark/micro-tmp-storage#readme",
  "now": {
    "alias": [
      "tmp.service.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "axios": "0.18.0",
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "micro-dev": "3.0.0",
    "nyc": "12.0.2",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "marked": "0.4.0",
    "micro": "9.3.2",
    "node-cache": "4.2.0",
    "uuid": "3.2.1",
    "winston": "2.4.3"
  }
}
