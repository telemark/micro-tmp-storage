  "homepage": "https://github.com/telemark/micro-tmp-storage",
  "now": {
    "alias": [
      "tmp.service.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "axios": "0.17.1",
    "ava": "0.24.0",
    "coveralls": "3.0.0",
    "nsp": "3.1.0",
